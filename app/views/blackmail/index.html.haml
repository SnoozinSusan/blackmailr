- 10.times do
  .blackmail
    .header
      .name @blackmail.victim_name
      .time
        Expires in
        %span.countdown{data: {'countdown-seconds' => 5.days.from_now.to_i}}
      .title @blackmail.title
    .main
      = image_tag 'sample.jpg'
      .description @blackmail.description
    .footer
.loading-more
  %div Loading more blackmail...
